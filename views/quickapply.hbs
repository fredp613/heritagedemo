						<th class="text-right" scope="col">Results</th>
						<th class="text-right" scope="col">Percentage</th>
					</tr>
					<tbody>
						<tr>
							<th scope="row">Eligible Criteria 1 (e.g. Organization serves indigenous communities</th>
							<td class="text-right">
								<span id="rsltA"></span>/
								<span id="rsltACount"></span>
							</td>
							<td class="text-right">(
								<span id="percA"></span>%)</td>
						</tr>
						<tr>
							<th scope="row">Eligiblity Criteria 2</th>
							<td class="text-right">
								<span id="rsltB"></span>/
								<span id="rsltBCount"></span>
							</td>
							<td class="text-right">(
								<span id="percB"></span>%)</td>
						</tr>
						<tr>
							<th scope="row">Eligiblity Criteria 3</th>
							<td class="text-right">
								<span id="rsltC"></span>/
								<span id="rsltCCount"></span>
							</td>
							<td class="text-right">(
								<span id="percC"></span>%)</td>
						</tr>
						<tr>
							<th scope="row">Eligiblity Criteria 4</th>
							<td class="text-right">
								<span id="rsltD"></span>/
								<span id="rsltDCount"></span>
							</td>
							<td class="text-right">(
								<span id="percD"></span>%)</td>
						</tr>
						<tr>
							<th scope="row">Eligiblity Criteria 5</th>
							<td class="text-right">
								<span id="rsltE"></span>/
								<span id="rsltECount"></span>
							</td>
							<td class="text-right">(
								<span id="percE"></span>%)</td>
						</tr>
					</tbody>
					<tbody>
						<tr class="active">
							<th scope="row">Eligiblity Criteria 6</th>
							<td class="text-right">
								<span id="itemsPassed"></span>/
								<span class="totalItems"></span>
							</td>
							<td class="text-right">(
								<span id="percPassed"></span>%)</td>
						</tr>
					</tbody>
					<tbody>
						<tr class="active">
							<th scope="row">Eligiblity Criteria  7</th>
							<td class="text-right">
								<span id="itemsEvaluated"></span>/
								<span class="totalItems"></span>
							</td>
							<td class="text-right">(
								<span id="percEval"></span>%)</td>
						</tr>
						<tr class="active">
							<th scope="row">Items
								<abbr title="Not applicable">N/A</abbr>
							</th>
							<td class="text-right">
								<span id="itemsNotApplicable"></span>/
								<span class="totalItems"></span>
							</td>
							<td class="text-right">(
								<span id="percNA"></span>%)</td>
						</tr>
					</tbody>
				</table>
				<p>
					<strong>Note:</strong> The number of total items passed means the total number of PASS and
					<abbr title="Not applicable">N/A</abbr>.</p>
